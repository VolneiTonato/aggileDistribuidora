    meta(http-equiv="X-UA-Compatible" content="IE=edge")

    block styleTemplate
        link(href="/public/libraries/node_modules/gentelella/vendors/bootstrap/dist/css/bootstrap.min.css", rel="stylesheet")
        link(href="/public/libraries/node_modules/gentelella/vendors/font-awesome/css/font-awesome.min.css", rel="stylesheet")
        link(href="/public/libraries/node_modules/gentelella/build/css/custom.min.css", rel="stylesheet")
        include ./override/style.css.pug

    block styleLocal
    
    
  body(class="nav-lg")
