//div(class="container")
    div(class="col-md-12")
        div(class="row")
            div(class="col-12")
                div(class="light-font")
                    ol(class="breadcrumb")
                        li(class="breadcrumb-item m3-3"): a(class="white-blue" href="#home") Voltar ao início

        div(class="row")
            <% if(typeof(catalogo[typeApresentacao]) != 'undefined'){ %>
            <% _.each(catalogo[typeApresentacao], function(item) { %>
                div(class!="col-md-4 col-sm-12 view-card-item" categoria-portifolio-galery!="<%= type %>")
                    div(class="card mb-3 text-center" style={'padding-top':'10px', 'min-width':'200px', 'height' : '300px'})
                        <% if(item[typeApresentacao + '_200x200']){%>
                        a(data-fancybox!="gallery-<%= type %>" href!="<%= item[typeApresentacao + '_800x800'] %>")
                            img(class="img-fluid rounded mx-auto lazy" src="/public/imagens/loading.gif" data-src!="<%= item[typeApresentacao + '_200x200'] %>" width="200px" height="200px" title!="" alt!="" style={'max-width':'100%', 'height':'100%'})
                        <% }else{ %>
                        img(class="img-fluid rounded mx-auto lazy" src="/public/imagens/loading.gif" width="200px" height="200px" data-src="data:image/jpeg;base64,")
                        <% } %>
                        
                        div(class="card-body")
            <% }) %>

            <% }else { %>
                div(class="col-12")
                    div(class="alert alert-danger" role="alert") 
                        |Oops, ocorreu um erro ao mostrar imagens :( 
                        <br />
                        |Clique aqui para voltar ao início
                        a(href="#home" class="btn btn-primary") aqui
            <% } %>