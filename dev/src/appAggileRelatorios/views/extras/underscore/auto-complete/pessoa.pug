script(type="template" id="template-autocomplete-cliente")
	p
		strong Razão Social: <%= cliente.razaoSocial %> | Fantasia: <%= cliente.nomeFantasia %>
		<br/>
		|Código: <%= id %> | CNPJ/CPF: <%= cliente.cnpjCpf %>
		<% if(telefones) {%>
		| | Telefone: <%= telefones.map((item) => { return item.numero }) %>
		<% } %>
		<% if('endereco' in window) {%>
		| | Endereço: <%= endereco.bairro %> | <%= endereco.logradouro %>
		| | Municipio: <%= endereco.municipio.descricao %> | UF: <%= endereco.municipio.estado.uf %>
		<% } %>


script(type="template" id="template-autocomplete-conta-bancaria")
	p
		strong Titular: <%= titular %>&nbsp;|&nbsp;
		<% if(['cliente','fabrica','cedente','vendedor'].indexOf(tipoCadastro) !== -1){ %>
		strong <%= _.startCase(tipoCadastro) %>: <%= _.get(pessoa, 'nome') %>
		<% } %>
		<br/>
		|Código: <%= id %> | CNPJ/CPF: <%= cnpjCpf%>
		| | Banco: <%= _.get(pessoaAgencia, 'agencia.banco.descricao') %>
		<br/>
		|Agência: <%= _.get(pessoaAgencia, 'agencia.nome') %> / Nº <%= _.get(pessoaAgencia, 'agencia.numero') %>
		<br/>
		<% if(telefones) {%>
		| | Telefone: <%= telefones.map((item) => { return item.numero }) %>
		<% } %>


script(type="template" id="template-autocomplete-cheque")
	p
		strong Número: <%= numero %>&nbsp;|&nbsp;
		strong Emissor (<%= _.startCase(tipoEmissor) %>): <%= _.get(emissor, 'nome') %>
		<br/>
		|Emissão: <%= createdAt %> | Data Pré Datado: <%= dataPreDatado %>
		<br />
		|Titular: <%= _.get(contaBancaria, 'titular') %>
		<br />
		|Banco: <%= _.get(contaBancaria, 'pessoaAgencia.agencia.banco.descricao') %>
		<br/>
		|Agência: <%= _.get(contaBancaria, 'pessoaAgencia.agencia.nome') %> / Nº <%= _.get(contaBancaria, 'pessoaAgencia.agencia.numero') %>


