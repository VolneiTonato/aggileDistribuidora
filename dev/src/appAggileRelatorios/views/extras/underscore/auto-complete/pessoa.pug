script(type="template" id="template-geral-autocomplete-pessoa")
	div.col-xs-12.col-md-12
		<%	var id = _.get(data, 'id', 'autocomplete-report-pessoa')	%>
		div.form-group.parent-autocomplete-pessoa
			label(for="Pessoa") <%= _.startCase(_.lowerCase(_.get(data, 'title'))) %>
			input(type="text" class="input-lg form-control autocomplete-vtt ui-autocomplete-input text-uppercase" id!="<%= id %>" placeholder!="<%= _.get(data, 'title') %>")
			span(style="display:none" class="span-message-autocomplete"): strong
			input(type="hidden" value="" name="report_pessoaId" id="report-pessoa-id")