{
    "presets": [
      [
        "@babel/preset-env", {
          "targets": "> 0.25%, not dead"
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      ["@babel/transform-runtime", {
          "absoluteRuntime": false,
          "corejs": false,
          "helpers": true,
          "regenerator": true,
          "useESModules": false
      }],
      "@babel/plugin-proposal-class-properties"
  ]
  }